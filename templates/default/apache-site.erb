<VirtualHost *:80>
  DocumentRoot /usr/share/vcl-web
  ServerName <%= node['vcl']['fqdn'] %>
</VirtualHost>
<VirtualHost *:443>
  SSLEngine on
  SSLCertificateFile /etc/httpd/ssl/server.crt
  SSLCertificateKeyFile /etc/httpd/ssl/server.key
  DocumentRoot /usr/share/vcl-web
  ServerName <%= node['vcl']['fqdn'] %>
</VirtualHost>
